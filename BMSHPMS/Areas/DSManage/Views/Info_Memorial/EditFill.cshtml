@model BMSHPMS.DSManage.ViewModels.Info_MemorialVMs.Info_MemorialVM

<wt:form vm="@Model">
    <wt:row items-per-row="ItemsPerRowEnum.Three">
        <wt:display field="Entity.Receipt.ReceiptNumber" />
        <wt:display field="Entity.SerialCode" />
        <wt:textbox field="Entity.Sum" />
        <wt:row>
            <wt:textbox field="Entity.DeceasedName_1" maxlength="12" search-url="/DSManage/Info_AutoComplete/GetContent" />
        </wt:row>
        <wt:row>
            <wt:textbox field="Entity.DeceasedName_2" maxlength="12" search-url="/DSManage/Info_AutoComplete/GetContent" />
        </wt:row>
        <wt:row>
            <wt:textbox field="Entity.DeceasedName_3" maxlength="12" search-url="/DSManage/Info_AutoComplete/GetContent" />
        </wt:row>
        <wt:row>
            <wt:textbox field="Entity.BenefactorName" maxlength="12" />
        </wt:row>
        <wt:row>
            <wt:textbox field="Entity.DSRemark" />
        </wt:row>
    </wt:row>
    <wt:hidden field="Entity.ID" />
    <wt:row align="AlignEnum.Right">
        <wt:submitbutton />
        <wt:closebutton />
    </wt:row>
</wt:form>
