@model BMSHPMS.DSManage.ViewModels.Info_DonorVMs.PrintPlaqueDonorVM

<div style="padding:20px;">
    @if (PrintPlaqueContext.Longevity_PrintPlaquePostList != null)
    {
        <wt:row items-per-row="ItemsPerRowEnum.Two">
            @foreach (var item in PrintPlaqueContext.Longevity_PrintPlaquePostList)
            {
                <wt:form vm="@Model" url="/DSManage/Info_Donor/ExportExcelTemplate2" old-post="true" target="_blank">
                    <wt:submitbutton text="@item.ButtonDisplayName" />

                    <wt:hidden field="WtmCacheKey" />
                    <wt:hidden field="PrintPlaquePostKey" default-value="@item.Key" />
                </wt:form>
            }
        </wt:row>
    }
    <hr />
    @if (PrintPlaqueContext.Memorial_PrintPlaquePostList != null)
    {
        <wt:row items-per-row="ItemsPerRowEnum.Two">
            @foreach (var item in PrintPlaqueContext.Memorial_PrintPlaquePostList)
            {
                <wt:form vm="@Model" url="/DSManage/Info_Donor/ExportExcelTemplate2" old-post="true" target="_blank">
                    <wt:submitbutton text="@item.ButtonDisplayName" />

                    <wt:hidden field="WtmCacheKey" />
                    <wt:hidden field="PrintPlaquePostKey" default-value="@item.Key" />
                </wt:form>
            }
        </wt:row>
    }

    @if (PrintPlaqueContext.Longevity_PrintPlaquePostList == null && PrintPlaqueContext.Memorial_PrintPlaquePostList == null)
    {
        <p>@nameof(PrintPlaqueContext.Longevity_PrintPlaquePostList) is null.</p>
    }
    <hr />
    <wt:row align="AlignEnum.Right">
        <wt:closebutton />
    </wt:row>
</div>