@model BMSHPMS.DSManage.ViewModels.Info_DonorVMs.InfoDonorCreateVM
@inject IStringLocalizer<Program> Localizer;

<wt:form vm="@Model">
    <wt:row items-per-row="ItemsPerRowEnum.Three">
        <wt:textbox field="ReceiptNumber" />
        <wt:combobox field="DharmaService" items="DharmaServiceSelectList" link-field="SumSelectDonationID" trigger-url="/dsmanage/info_donor/GetDonationSelectListByDharmaServiceID" />
        <wt:combobox field="SumSelectDonationID" />
    </wt:row>
    <wt:row items-per-row="ItemsPerRowEnum.One">
        <wt:textbox field="DeceasedName_1" maxlength="12" search-url="/DSManage/Info_AutoComplete/GetContent" />
    </wt:row>
    <wt:row items-per-row="ItemsPerRowEnum.One">
        <wt:textbox field="DeceasedName_2" maxlength="12" search-url="/DSManage/Info_AutoComplete/GetContent" />
    </wt:row>
    <wt:row items-per-row="ItemsPerRowEnum.One">
        <wt:textbox field="DeceasedName_3" maxlength="12" search-url="/DSManage/Info_AutoComplete/GetContent" />
    </wt:row>
    <wt:row items-per-row="ItemsPerRowEnum.One">
        <wt:textbox field="BenefactorName" />
    </wt:row>
    <wt:row items-per-row="ItemsPerRowEnum.One">  
        <wt:textbox field="LongevityName" />
    </wt:row>
    <wt:row items-per-row="ItemsPerRowEnum.One">
        <wt:textbox field="DSRemark" />
    </wt:row>
    <wt:row align="AlignEnum.Right">
        <wt:submitbutton />
        <wt:closebutton />
    </wt:row>
</wt:form>
