@model BMSHPMS.DSManage.ViewModels.DSDonationProjectVMs.DSDonationProjectVM
@inject IStringLocalizer<Program> Localizer;

<wt:form vm="@Model">
    <wt:row items-per-row="ItemsPerRowEnum.Two">
        <wt:textbox field="Entity.Sum" />
        <wt:textbox field="Entity.SerialCode" />
        <wt:combobox field="Entity.DonationCategory" items="AllDSDonationCategory" />
        <wt:combobox field="Entity.DServiceProjID" items="AllDServiceProjs" />
    </wt:row>
    <wt:hidden field="Entity.ID" />
    <wt:row align="AlignEnum.Right">
        <wt:submitbutton />
        <wt:closebutton />
    </wt:row>
</wt:form>
