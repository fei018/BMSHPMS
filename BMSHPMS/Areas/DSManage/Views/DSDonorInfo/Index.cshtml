    <wt:row items-per-row="ItemsPerRowEnum.Three">
        <wt:textbox field="Searcher.SerialCode" />
        <wt:textbox field="Searcher.Sum" />
        <wt:textbox field="Searcher.LongevityName" />
        <wt:textbox field="Searcher.BenefactorName" />      
