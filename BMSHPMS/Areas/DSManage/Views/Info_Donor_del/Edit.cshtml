@model BMSHPMS.DSManage.ViewModels.Info_Donor_delVMs.Info_Donor_delVM
@inject IStringLocalizer<Program> Localizer;

<wt:form vm="@Model">
    <wt:row items-per-row="ItemsPerRowEnum.Two">
        <wt:textbox field="Entity.LongevityName" />
        <wt:textbox field="Entity.DeceasedName_1" maxlength="12" search-url="/DSManage/Info_AutoComplete/GetContent" />
        <wt:textbox field="Entity.DeceasedName_2" maxlength="12" search-url="/DSManage/Info_AutoComplete/GetContent" />
        <wt:textbox field="Entity.DeceasedName_3" maxlength="12" search-url="/DSManage/Info_AutoComplete/GetContent" />
        <wt:textbox field="Entity.BenefactorName" />
        <wt:textbox field="Entity.Sum" />
        <wt:textbox field="Entity.SerialCode" />
        <wt:textbox field="Entity.DSRemark" />
        <wt:combobox field="Entity.Receipt_delID" items="AllReceipt_dels" />
    </wt:row>
    <wt:hidden field="Entity.ID" />
    <wt:row align="AlignEnum.Right">
        <wt:submitbutton />
        <wt:closebutton />
    </wt:row>
</wt:form>
