@model BMSHPMS.DSManage.ViewModels.DSReceiptInfoVMs.DSReceiptInfoVM
@inject IStringLocalizer<Program> Localizer;

<wt:form vm="@Model">
<wt:row items-per-row="ItemsPerRowEnum.Two">
<wt:textbox field="Entity.ReceiptNumber" />
<wt:textbox field="Entity.ReceiptOwn" />
<wt:textbox field="Entity.ContactName" />
<wt:textbox field="Entity.ContactPhone" />
<wt:textbox field="Entity.Sum" />
<wt:textbox field="Entity.DSRemark" />
<wt:datetime field="Entity.ReceiptDate" />
</wt:row>
    <wt:row align="AlignEnum.Right">
        <wt:submitbutton />
        <wt:closebutton />
    </wt:row>
</wt:form>
