@model BMSHPMS.DSManage.ViewModels.Opt_DharmaServiceVMs.Opt_DharmaServiceVM
@inject IStringLocalizer<Program> Localizer;

<wt:form vm="@Model">
    <wt:quote>@Localizer["Sys.DeleteConfirm"]</wt:quote>
    <wt:row items-per-row="ItemsPerRowEnum.Two">
        <wt:display field="Entity.ServiceName" />
        <wt:display field="Entity.SerialCode" />
        <wt:display field="Entity.ServiceDateDescription" />
        <wt:display field="Entity.ServiceOrganizer" />
        <wt:display field="Entity.Enable" />
    </wt:row>
    <wt:hidden field="Entity.ID" />
    <wt:row align="AlignEnum.Right">
        <wt:submitbutton theme=" ButtonThemeEnum.Warm" text="@Localizer["Sys.Delete"]" />
        <wt:closebutton />
    </wt:row>
</wt:form>
