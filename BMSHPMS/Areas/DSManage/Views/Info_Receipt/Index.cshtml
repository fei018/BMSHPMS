@model BMSHPMS.DSManage.ViewModels.Info_ReceiptVMs.Info_ReceiptListVM

<wt:searchpanel vm="@Model" reset-btn="true" >
    <wt:row items-per-row="ItemsPerRowEnum.Four">
        <wt:datetime field="Searcher.DharmaServiceYear" type="Year"/>
        <wt:combobox field="Searcher.DharmaServiceName" items="Searcher.AllDharmaServiceName" />
        <wt:textbox field="Searcher.ReceiptNumber" />
        @*<wt:textbox field="Searcher.Sum" /> *@
        <wt:textbox field="Searcher.ReceiptOwn" />
        <wt:textbox field="Searcher.ContactName" />
        <wt:textbox field="Searcher.ContactPhone" />
        <wt:datetime field="Searcher.ReceiptDate" type="Date" range="true"/>
    </wt:row>
</wt:searchpanel>
<wt:grid vm="@Model" url="/DSManage/Info_Receipt/Search" hidden-grid-index limit="50" limits="new int[]{50,100,200,500,1000,10000}" />