@model BMSHPMS.DSManage.ViewModels.Opt_DonationProjectVMs.Opt_DonationProjectVM
@inject IStringLocalizer<Program> Localizer;

<wt:form vm="@Model">
    <wt:row items-per-row="ItemsPerRowEnum.Two">
        <wt:combobox field="Entity.DharmaServiceID" items="AllOpt_DharmaService" disabled />
        <wt:combobox field="Entity.DonationCategory" items="AllOpt_DonationCategory" disabled />
        <wt:textbox field="Entity.SerialCode" />
        <wt:textbox field="Entity.Sum" />     
    </wt:row>
    <wt:row items-per-row="ItemsPerRowEnum.Three">
        <wt:display field="Entity.UsedNumber" />
    </wt:row>
    <wt:hidden field="Entity.ID" />
    <wt:row align="AlignEnum.Right">
        <wt:submitbutton />
        <wt:closebutton />
    </wt:row>
</wt:form>
